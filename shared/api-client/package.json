{
  "name": "@evscrap/api-client",
  "version": "0.1.0",
  "description": "evscrap OpenAPI typed client (openapi-typescript + openapi-fetch)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "generate:openapi": "openapi-typescript ../../core-api/openapi.yaml -o src/generated/openapi-types.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.8"
  },
  "devDependencies": {
    "openapi-typescript": "^7.6.0",
    "typescript": "^5.7.0"
  }
}
